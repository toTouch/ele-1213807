<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiliulou.electricity.mapper.BoxOtherPropertiesMapper">
    
    <resultMap type="com.xiliulou.electricity.entity.BoxOtherProperties" id="BoxOtherPropertiesMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="electricityCabinetId" column="electricity_cabinet_id" jdbcType="INTEGER"/>
        <result property="cellNo" column="cell_no" jdbcType="VARCHAR"/>
        <result property="delFlag" column="del_flag" jdbcType="INTEGER"/>
        <result property="remark" column="remark" jdbcType="VARCHAR"/>
        <result property="lockReason" column="lock_reason" jdbcType="INTEGER"/>
        <result property="lockStatusChangeTime" column="lock_status_change_time" jdbcType="INTEGER"/>
        <result property="createTime" column="create_time" jdbcType="INTEGER"/>
        <result property="updateTime" column="update_time" jdbcType="INTEGER"/>
    </resultMap>
    
    <!-- 基础字段 -->
    <sql id="selectBoxOtherProperties">
       	select   id, electricity_cabinet_id, cell_no, del_flag, remark, lock_reason, lock_status_change_time, create_time, update_time        from t_box_other_properties
    </sql>
    
    <!--查询单个-->
    <select id="selectById" resultMap="BoxOtherPropertiesMap">
        <include refid="selectBoxOtherProperties"/>
        where id = #{id}
    </select>
    
    <!--查询指定行数据-->
    <select id="selectByPage" resultMap="BoxOtherPropertiesMap">
        <include refid="selectBoxOtherProperties"/>
        limit #{offset}, #{limit}
    </select>
    
    <!--通过实体作为筛选条件查询-->
    <select id="selectByQuery" resultMap="BoxOtherPropertiesMap">
        <include refid="selectBoxOtherProperties"/>
        <where>
            <if test="id != null">
                and id = #{id}
            </if>
            <if test="electricityCabinetId != null">
                and electricity_cabinet_id = #{electricityCabinetId}
            </if>
            <if test="cellNo != null and cellNo != ''">
                and cell_no = #{cellNo}
            </if>
            <if test="delFlag != null">
                and del_flag = #{delFlag}
            </if>
            <if test="remark != null and remark != ''">
                and remark = #{remark}
            </if>
            <if test="lockReason != null">
                and lock_reason = #{lockReason}
            </if>
            <if test="lockStatusChangeTime != null">
                and lock_status_change_time = #{lockStatusChangeTime}
            </if>
            <if test="createTime != null">
                and create_time = #{createTime}
            </if>
            <if test="updateTime != null">
                and update_time = #{updateTime}
            </if>
        </where>
    </select>
    
    <!--新增所有列-->
    <insert id="insertOne" keyProperty="id" useGeneratedKeys="true">
        insert into t_box_other_properties(electricity_cabinet_id, cell_no, del_flag, remark, lock_reason, lock_status_change_time, create_time, update_time)
        values (#{electricityCabinetId}, #{cellNo}, #{delFlag}, #{remark}, #{lockReason}, #{lockStatusChangeTime},#{createTime}, #{updateTime})
    </insert>
    
    <!--新增所有列-->
    <insert id="insertBoxOtherProperties" keyProperty="id" useGeneratedKeys="true">
        insert into t_box_other_properties
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="electricityCabinetId != null">electricity_cabinet_id,</if>
            <if test="cellNo != null">cell_no,</if>
            <if test="delFlag != null">del_flag,</if>
            <if test="remark != null ">remark,</if>
            <if test="lockReason != null">lock_reason,</if>
            <if test="lockStatusChangeTime != null">lock_status_change_time,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="electricityCabinetId != null">#{electricityCabinetId},</if>
            <if test="cellNo != null">#{cellNo},</if>
            <if test="delFlag != null">#{delFlag},</if>
            <if test="remark != null ">#{remark},</if>
            <if test="lockReason != null">#{lockReason},</if>
            <if test="lockStatusChangeTime != null">#{lockStatusChangeTime},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
        </trim>
        on duplicate key
        update
        <if test="remark != null ">remark=#{remark},</if>
        <if test="lockReason != null">lock_reason=#{lockReason},</if>
        <if test="lockStatusChangeTime != null">lock_status_change_time=#{lockStatusChangeTime},</if>
        <if test="updateTime != null">update_time=#{updateTime}</if>
    </insert>
    
    <!--通过主键修改数据-->
    <update id="update">
        update t_box_other_properties
        <set>
            <if test="electricityCabinetId != null">
                electricity_cabinet_id = #{electricityCabinetId},
            </if>
            <if test="cellNo != null and cellNo != ''">
                cell_no = #{cellNo},
            </if>
            <if test="delFlag != null">
                del_flag = #{delFlag},
            </if>
            <if test="remark != null and remark != ''">
                remark = #{remark},
            </if>
            <if test="lockReason != null">
                lock_reason = #{lockReason},
            </if>
            <if test="lockStatusChangeTime != null">
                lock_status_change_time = #{lockStatusChangeTime},
            </if>
            <if test="createTime != null">
                create_time = #{createTime},
            </if>
            <if test="updateTime != null">
                update_time = #{updateTime},
            </if>
        </set>
        where id = #{id}
    </update>
    
    <!--通过主键删除-->
    <delete id="deleteById">
        delete from t_box_other_properties where id = #{id}
    </delete>

</mapper>
