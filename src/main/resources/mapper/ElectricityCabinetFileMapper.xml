<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiliulou.electricity.mapper.ElectricityCabinetFileMapper">

    <delete id="deleteByDeviceInfo">
        delete from t_electricity_cabinet_file
        where type = #{fileType} and is_oss=#{isUseOSS}
        <if test="otherId != null">
            and other_id = #{otherId}
        </if>
    </delete>


</mapper>
