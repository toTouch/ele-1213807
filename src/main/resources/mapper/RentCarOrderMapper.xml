    <!-- 基础字段 -->
    <sql id="selectRentCarOrder">
       	select   id, order_id, uid, name, phone, car_sn, car_deposit, car_model_id, type,transaction_type, status, remark, create_time, update_time, store_id, franchisee_id, tenant_id        from t_rent_car_order
    </sql>

    <!--查询单个-->
    <select id="selectById" resultMap="RentCarOrderMap">
        <include refid="selectRentCarOrder"/>
        where id = #{id}
    </select>

    <!--查询指定行数据-->
    <select id="selectByPage" resultMap="RentCarOrderMap">
        <include refid="selectRentCarOrder"/>
        <where>
            <if test="orderId != null and orderId != ''">
                and order_id = #{orderId}
            </if>
            <if test="name != null and name != ''">
                and name = #{name}
            </if>
            <if test="phone != null and phone != ''">
                and phone = #{phone}
            </if>
            <if test="carSn != null and carSn != ''">
