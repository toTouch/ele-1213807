<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiliulou.electricity.mapper.asset.AssetWarehouseDetailMapper">
    <!-- 基础字段 -->
    <sql id="basic_field">
        id,
        record_no,
        warehouse_id,
        type,
        sn,
        tenant_id,
        del_flag,
        create_time,
        update_time,
        remark
    </sql>
    
    <!--新增所有列-->
    <insert id="insertOne" keyProperty="id" useGeneratedKeys="true">
        insert into t_asset_warehouse_detail
        <trim prefix="(" suffix=")" suffixOverrides=",">
            record_no,
            warehouse_id,
            type,
            sn,
            tenant_id,
            del_flag,
            create_time,
            update_time,
            remark
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            #{recordNo},
            #{warehouseId},
            #{type},
            #{sn},
            #{tenantId},
            #{delFlag},
            #{createTime},
            #{updateTime},
            #{remark}
        </trim>
    </insert>
    
    <insert id="batchInsert" keyProperty="id" useGeneratedKeys="true">
        insert into t_asset_warehouse_detail(record_no,warehouse_id,type,sn,tenant_id,del_flag,create_time,update_time,remark)
        values
        <foreach item="item" index="index" collection="batchInsertList" separator=",">
            (#{item.recordNo},#{item.warehouseId},#{item.type}, #{item.sn}, #{item.tenantId}, #{item.delFlag}, #{item.createTime}, #{item.updateTime}, #{item.remark})
        </foreach>
    </insert>
    
    <select id="selectListBySn" resultType="com.xiliulou.electricity.entity.asset.AssetWarehouseDetail">
        SELECT <include refid="basic_field"/>
        FROM t_asset_warehouse_detail
        <where>
            <if test="sn != null and sn != ''">
                and sn like concat('%', #{sn}, '%')
            </if>
            and warehouse_id = #{warehouseId} and del_flag = 0
        </where>
    </select>
    
    <select id="selectListByRecordNo" resultType="com.xiliulou.electricity.entity.asset.AssetWarehouseDetail">
        SELECT <include refid="basic_field"/>
        FROM t_asset_warehouse_detail
        WHERE del_flag = 0 and record_no = #{recordNo}
    </select>
    
    <select id="selectListByRecordNoList" resultType="com.xiliulou.electricity.entity.asset.AssetWarehouseDetail">
        SELECT <include refid="basic_field"/>
        FROM t_asset_warehouse_detail
        <where>
            del_flag = 0
            <if test="recordNoList != null and recordNoList.size() > 0">
                and record_no in
                <foreach collection="recordNoList" item="recordNo" index="index" separator="," open="(" close=")">
                    #{recordNo}
                </foreach>
            </if>
        </where>
    </select>

</mapper>




