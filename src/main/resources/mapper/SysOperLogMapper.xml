<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiliulou.electricity.mapper.SysOperLogMapper">
    
    <resultMap type="com.xiliulou.electricity.entity.SysOperLog" id="SysOperLogMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="title" column="title" jdbcType="VARCHAR"/>
        <result property="requestId" column="request_id" jdbcType="VARCHAR"/>
        <result property="operatorUid" column="operator_uid" jdbcType="INTEGER"/>
        <result property="operIp" column="oper_ip" jdbcType="VARCHAR"/>
        <result property="status" column="status" jdbcType="INTEGER"/>
        <result property="errorMsg" column="error_msg" jdbcType="VARCHAR"/>
        <result property="operTime" column="oper_time" jdbcType="INTEGER"/>
        <result property="tenantId" column="tenant_id" jdbcType="INTEGER"/>
        <result property="attr" column="attr" jdbcType="VARCHAR"/>
    </resultMap>
    
    <!-- 基础字段 -->
    <sql id="selectSysOperLog">
       	select   id, title, request_id, operator_uid, oper_ip, status, error_msg, oper_time, tenant_id, attr        from t_sys_oper_log
    </sql>
    
    <!--查询单个-->
    <select id="selectById" resultMap="SysOperLogMap">
        <include refid="selectSysOperLog"/>
        where id = #{id}
    </select>
    
    <!--查询指定行数据-->
    <select id="selectByPage" resultMap="SysOperLogMap">
        <include refid="selectSysOperLog"/>
        <where>
            <if test="title != null and title != ''">
                and title like concat('%',#{title},'%')
            </if>
            <if test="operatorUid != null">
                and operator_uid = #{operatorUid}
            </if>
            <if test="status != null">
                and status = #{status}
            </if>
            <if test="tenantId != null">
                and tenant_id = #{tenantId}
            </if>
            <if test="beginTime != null">
                and oper_time &gt;=#{beginTime}
            </if>
            <if test="endTime != null">
                and oper_time &lt;=#{endTime}
            </if>
        </where>
        order by oper_time desc
        limit #{offset}, #{size}
    </select>
    
    
    <select id="pageCount" resultType="int">
       select count(1)   from t_sys_oper_log
        <where>
            <if test="title != null and title != ''">
                and title like concat('%',#{title},'%')
            </if>
            <if test="operatorUid != null">
                and operator_uid = #{operatorUid}
            </if>
            <if test="status != null">
                and status = #{status}
            </if>
            <if test="tenantId != null">
                and tenant_id = #{tenantId}
            </if>
            <if test="beginTime != null">
                and oper_time &gt;=#{beginTime}
            </if>
            <if test="endTime != null">
                and oper_time &lt;=#{endTime}
            </if>
        </where>
    </select>
    
    <!--通过实体作为筛选条件查询-->
    <select id="selectByQuery" resultMap="SysOperLogMap">
        <include refid="selectSysOperLog"/>
        <where>
            <if test="id != null">
                and id = #{id}
            </if>
            <if test="title != null and title != ''">
                and title = #{title}
            </if>
            <if test="requestId != null and requestId != ''">
                and request_id = #{requestId}
            </if>
            <if test="operatorUid != null">
                and operator_uid = #{operatorUid}
            </if>
            <if test="operIp != null and operIp != ''">
                and oper_ip = #{operIp}
            </if>
            <if test="status != null">
                and status = #{status}
            </if>
            <if test="errorMsg != null and errorMsg != ''">
                and error_msg = #{errorMsg}
            </if>
            <if test="operTime != null">
                and oper_time = #{operTime}
            </if>
            <if test="tenantId != null">
                and tenant_id = #{tenantId}
            </if>
            <if test="attr != null and attr != ''">
                and attr = #{attr}
            </if>
        </where>
    </select>
    
    <!--新增所有列-->
    <insert id="insertOne" keyProperty="id" useGeneratedKeys="true">
        insert into t_sys_oper_log(title, request_id, operator_uid, oper_ip, status, error_msg, oper_time, tenant_id, attr)
        values (#{title}, #{requestId}, #{operatorUid}, #{operIp}, #{status}, #{errorMsg}, #{operTime}, #{tenantId}, #{attr})
    </insert>
    
    <!--通过主键修改数据-->
    <update id="update">
        update t_sys_oper_log
        <set>
            <if test="title != null and title != ''">
                title = #{title},
            </if>
            <if test="requestId != null and requestId != ''">
                request_id = #{requestId},
            </if>
            <if test="operatorUid != null">
                operator_uid = #{operatorUid},
            </if>
            <if test="operIp != null and operIp != ''">
                oper_ip = #{operIp},
            </if>
            <if test="status != null">
                status = #{status},
            </if>
            <if test="errorMsg != null and errorMsg != ''">
                error_msg = #{errorMsg},
            </if>
            <if test="operTime != null">
                oper_time = #{operTime},
            </if>
            <if test="tenantId != null">
                tenant_id = #{tenantId},
            </if>
            <if test="attr != null and attr != ''">
                attr = #{attr},
            </if>
        </set>
        where id = #{id}
    </update>
    
    <!--通过主键删除-->
    <delete id="deleteById">
        delete from t_sys_oper_log where id = #{id}
    </delete>

</mapper>
