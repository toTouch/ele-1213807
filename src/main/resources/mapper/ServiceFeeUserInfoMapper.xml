<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiliulou.electricity.mapper.ServiceFeeUserInfoMapper">

    <update id="update">
        update t_service_fee_user_info
        <set>
            <if test="existBatteryServiceFee != null">
                exist_battery_service_fee = #{existBatteryServiceFee},
            </if>
            <if test="disableMemberCardNo != null">
                disable_member_card_no = #{disableMemberCardNo},
            </if>
        </set>
        where id = #{id} and tenant_id = #{tenantId}
    </update>

</mapper>
