<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiliulou.electricity.mapper.FyConfigServiceMapper">
    
    
    <select id="selectByTenantId" resultType="com.xiliulou.electricity.entity.FyConfig">
        select id,
               tenant_id,
               store_code,
               merchant_code,
               channel_code,
               del_flag,
               create_time,
               update_time
        from t_fy_config
        where del_flag = 0
          and tenant_id = #{tenantId}
    </select>
</mapper>
