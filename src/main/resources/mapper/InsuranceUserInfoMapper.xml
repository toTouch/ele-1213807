<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiliulou.electricity.mapper.InsuranceUserInfoMapper">



    <update id="update">
        update t_insurance_user_info
        <set>
            <if test="franchiseeId != null">
                franchisee_id = #{franchiseeId},
            </if>
            <if test="cid != null">
                cid = #{cid},
            </if>
            <if test="premium != null">
                premium = #{premium},
            </if>
            <if test="forehead != null">
                forehead = #{forehead},
            </if>
            <if test="insuranceId != null">
                insurance_id = #{insuranceId},
            </if>
            <if test="insuranceExpireTime != null">
                insurance_expire_time = #{insuranceExpireTime},
            </if>
            <if test="isUse != null">
                is_use = #{isUse},
            </if>
            <if test="updateTime != null">
                update_time = #{updateTime},
            </if>
            <if test="delFlag != null">
                del_flag = #{delFlag},
            </if>
        </set>
        where uid = #{uid} and tenant_id = #{tenantId}
    </update>

</mapper>
